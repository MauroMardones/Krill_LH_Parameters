@techreport{Taylor2016,
abstract = {11 Electronic LEngth Frequency ANalysis (ELEFAN) is a system of stock assessment methods based on 12 length-frequency (LFQ) data. One step is the estimation of growth from the progression of LFQ 13 modes through time using the von Bertalanffy growth function (VBGF). The option to fit a seasonally-14 oscillating von Bertalanffy growth function (soVBGF) requires a more intensive search due to two 15 additional parameters. This work describes an R-based implementation of the fitting procedure using 16 two new optimization approaches ('simulated annealing' and 'genetic algorithm'). The performance 17 of the genetic algorithm approach in soVBGF parameter estimation was evaluated against a 18 generated LFQ data set with known values. The results indicate that the optimization approach is 19 capable of finding high scoring solutions, yet initial settings regarding the initial restructuring process 20 for LFQ bin scoring (i.e. 'moving average', MA) and the fixing of the infinite length parameter (L∞) 21 both have significant effects on parameter estimation error. An outlook provides context on the 22 significance of the R-based implementation for further testing and development, and discusses its 23 future relevance for data-limited assessment. 24 25 Keywords 26 growth model, von Bertalanffy growth function, length-frequency data, ELEFAN, data-limited stock 27 assessment 28 Working Paper (v. 2016.12.05)},
author = {Taylor, Marc H and Mildenberger, Tobias},
file = {:Users/mauriciomardones/IFOP/Cursos{\_}Congresos/Curso TropFish{\_}SantaMarta{\_}2017/literature/Taylor{\_}Mildenberger{\_}Extending{\_}ELEFAN{\_}in{\_}R{\_}v20161205.pdf:pdf},
mendeley-groups = {DOCAS/LH{\_}Parameters},
title = {{Extending ELEFAN in R}},
url = {https://cran.r-project.org},
year = {2016}
}
